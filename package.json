{
  "name": "eaas-platform",
  "version": "1.0.0",
  "description": "Evaluation as a Service - AI Model Evaluation Platform",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3002",
    "build": "next build",
    "start": "next start --port 3002",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "start:production": "NODE_ENV=production npm start",
    "docker:build": "docker build -t eaas-platform .",
    "docker:run": "docker run -p 3002:3002 --env-file .env.production eaas-platform",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:deploy": "supabase db push",
    "api:dev": "cd api && python3 main.py",
    "setup": "npm install && cd api && pip install -r requirements.txt",
    "setup:production": "npm ci --production",
    "dev:all": "concurrently \"npm run supabase:start\" \"redis-server\" \"npm run api:dev\" \"npm run dev\"",
    "health-check": "curl -f http://localhost:3002/api/health || exit 1",
    "precommit": "npm run lint && npm run type-check && npm run test"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.2",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "googleapis": "^161.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.4",
    "nodemailer": "^7.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "supabase": "^2.48.3",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  },
  "keywords": [
    "ai-evaluation",
    "model-testing",
    "rubrics",
    "automation",
    "nextjs",
    "supabase",
    "fastapi"
  ],
  "author": "EaaS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/eaas-platform.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
