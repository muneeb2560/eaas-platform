{
  "name": "eaas-platform",
  "version": "1.0.0",
  "description": "Evaluation as a Service - AI Model Evaluation Platform",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3002",
    "build": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=64' next build",
    "build:memory-safe": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=1024 --max-semi-space-size=64 --optimize-for-size' next build",
    "start": "node start.js",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:production": "NODE_ENV=production npm start",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:deploy": "supabase db push",
    "api:dev": "cd api && python3 main.py",
    "setup": "npm install && cd api && pip install -r requirements.txt",
    "dev:all": "concurrently \"npm run supabase:start\" \"redis-server\" \"npm run api:dev\" \"npm run dev\"",
    "health-check": "node scripts/health-check.js",
    "health-check:local": "node scripts/health-check.js http://localhost:3002/api/health",
    "health-check:production": "node scripts/health-check.js ${RAILWAY_STATIC_URL:-http://localhost:3000}/api/health",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:full": "rm -rf .next node_modules package-lock.json && npm install",
    "precommit": "npm run lint && npm run type-check && npm run test"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.2",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.4",
    "nodemailer": "^7.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4",
    "googleapis": "^161.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^30.2.0",
    "supabase": "^2.48.3"
  },
  "overrides": {
    "glob": "^11.0.0",
    "rimraf": "^6.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "ajv": "^8.17.1",
    "uri-js": "^4.4.1",
    "whatwg-url": "^11.0.0",
    "tr46": "^4.1.1"
  },
  "keywords": [
    "ai-evaluation",
    "model-testing",
    "rubrics",
    "automation",
    "nextjs",
    "supabase",
    "fastapi"
  ],
  "author": "EaaS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/eaas-platform.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
